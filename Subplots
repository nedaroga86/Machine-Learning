    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Create a sample dataset\n",
    "data = {\n",
    "    'Category A': [10, 20, 30, 40],\n",
    "    'Category B': [20, 30, 10, 50],\n",
    "    'Category C': [30, 10, 20, 60]\n",
    "}\n",
    "index = ['Q1', 'Q2', 'Q3', 'Q4']\n",
    "\n",
    "df = pd.DataFrame(data, index=index)\n",
    "\n",
    "# Create subplots\n",
    "fig, ax = plt.subplots(1, 3, figsize=(15, 5))\n",
    "\n",
    "# Plot each category as a separate bar chart in a subplot\n",
    "df['Category A'].plot(kind='bar', ax=ax[0], title='Category A', color='blue')\n",
    "df['Category B'].plot(kind='bar', ax=ax[1], title='Category B', color='green')\n",
    "df['Category C'].plot(kind='bar', ax=ax[2], title='Category C', color='red')\n",
    "\n",
    "# Show the plot\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
